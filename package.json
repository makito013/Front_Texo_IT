{
  "name": "teste-front",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "prepare": "git config --global --add safe.directory ./ & ((chmod ug+x .husky/* & chmod ug+x .git/hooks/* & echo 'is a mac') || echo 'is not mac') & husky install",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "build": "tsc && vite build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "deploy:dev:application": "npm run build && aws s3 sync dist s3://fiscalmax-development-frontend-clients/consultant/application && aws cloudfront create-invalidation --distribution-id E4M12H1J191SF --paths '/*'",
    "deploy:dev:storybook": "npm run build-storybook && aws s3 sync storybook-static s3://fiscalmax-development-frontend-clients/consultant/storybook && aws cloudfront create-invalidation --distribution-id E2YAPTKHNXDJS4 --paths '/*'"
  },
  "dependencies": {
    "@commitlint/cli": "^19.0.3",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@formkit/auto-animate": "^0.8.1",
    "@mui/icons-material": "^5.15.14",
    "@mui/material": "^5.15.14",
    "axios": "^1.6.7",
    "formik": "^2.4.5",
    "install": "^0.13.0",
    "localforage": "^1.10.0",
    "match-sorter": "^6.3.4",
    "npm": "^10.5.0",
    "oidc-client-ts": "^3.0.1",
    "oidc-react": "^3.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-query": "^3.39.3",
    "react-router-dom": "^6.22.2",
    "sort-by": "^1.2.0",
    "styled-components": "^6.1.8",
    "yup": "^1.4.0"
  },
  "devDependencies": {
    "@commitlint/config-conventional": "^19.0.3",
    "@storybook/addon-essentials": "^7.6.17",
    "@storybook/addon-interactions": "^7.6.17",
    "@storybook/addon-links": "^7.6.17",
    "@storybook/addon-onboarding": "^1.0.11",
    "@storybook/addon-styling-webpack": "^0.0.6",
    "@storybook/blocks": "^7.6.17",
    "@storybook/react": "^7.6.17",
    "@storybook/react-vite": "^7.6.17",
    "@storybook/test": "^7.6.17",
    "@types/react": "^18.2.56",
    "@types/react-dom": "^18.2.19",
    "@typescript-eslint/eslint-plugin": "^7.0.2",
    "@typescript-eslint/parser": "^7.0.2",
    "@vitejs/plugin-react": "^4.2.1",
    "commitizen": "^4.3.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.5",
    "eslint-plugin-storybook": "^0.8.0",
    "husky": "^9.0.11",
    "prettier": "^3.2.5",
    "storybook": "^7.6.17",
    "typescript": "^5.2.2",
    "vite": "^5.1.4"
  },
  "lint-staged": {
    "**/*": "prettier --write --ignore-unknown"
  },
  "resolutions": {
    "styled-components": "^5"
  }
}
